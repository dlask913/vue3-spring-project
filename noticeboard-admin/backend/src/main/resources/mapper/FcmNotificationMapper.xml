<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.limnj.noticeboardadmin.fcm.FcmNotificationMapper">
    <insert id="saveFcmToken" parameterType="FcmTokenRequestDto">
        INSERT INTO fcm_tokens(user_id, token, device_type)
        VALUES (#{userId}, #{token}, #{deviceType})
    </insert>
</mapper>